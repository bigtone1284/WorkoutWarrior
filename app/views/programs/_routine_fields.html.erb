<div class="nested-fields">
  <% @exercises = Exercise.all %>
  <div class="col-md-3"><%= f.input :exercise_id, :collection => @exercises, :label_method => :title, :value_method => :id, :label => "Exercises", id: "categories-div" %></div>
  <div class="col-md-2" id="sets"><%= f.input :sets %></div>
  <div class="col-md-2" id="reps"><%= f.input :reps %></div>
  <div class="col-md-2"><%= f.input :duration %></div>
  <div class="col-md-3"><%= f.input :instructions %></div>

  <%= link_to_remove_association "remove exercise", f, class: 'btn btn-danger btn-xs' %>
</div>
